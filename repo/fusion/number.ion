(module number '/fusion/private/builtins'

  '''
Operations on numbers (int, decimal, and float).
  '''

  (require "/fusion/experimental/defines")

  (require "/fusion/private/compare")
  (provide < <= = >= >)


  (defpub_j  +  "com.amazon.fusion.SumProc")
  (defpub_j  -  "com.amazon.fusion.DifferenceProc")
  (defpub_j  *  "com.amazon.fusion.ProductProc")
  (defpub_j  /  "com.amazon.fusion.DivideProc")

)
