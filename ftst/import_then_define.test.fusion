(require "/fusion/experimental/check")


// Test that we can define a name that's already imported.
//  That's legal at top-level, but not at module-level

(require "/module/dupe1")
(define (d1) dupe)

(check === 1 dupe)
(check === 1 (d1))

(require "/module/dupe2")
(define (d2) dupe)

(check === 2 dupe)
(check === 1 (d1))
(check === 2 (d2))

(define dupe 4)
(check === 4 dupe)
(check === 1 (d1))
(check === 2 (d2))

(define (d_local) dupe)

(define dupe 5)
(check === 5 dupe)
(check === 5 (d_local))

(require "/module/dupe2")
(check === 2 dupe)
(check === 1 (d1))
(check === 2 (d2))
(check === 5 (d_local))
